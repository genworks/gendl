@subsubsection Parallel-dimension

@b{Description}

Creates a dimension annotation along an axis from a start point to an end point.

@noindent @b{Computed-slots}

@b{:base-plane-normal}

@itemize {}

 @item Must be specified in the subclass except for angular

@end itemize

@b{:dim-text-start}

@itemize {}

 @item 3D Point. Determines where the text will start. Defaults to reasonable location for 
horizontal-dimension.

@end itemize

@b{:leader-direction-1-vector}

@itemize {}

 @item Must be specified in the subclass except for angular

@end itemize

@b{:leader-direction-2-vector}

@itemize {}

 @item Must be specified in the subclass except for angular

@end itemize

@b{:witness-direction-vector}

@itemize {}

 @item Must be specified in the subclass except for angular

@end itemize

@noindent @b{Examples}

@smallexample 
@cartouche        

 (in-package :gdl-user)
                   
 (define-object  parallel-dimension-sample (base-object)
  
  
   :objects
   ((box :type 'box
         :length 10 :width (* (the-child length) +phi+)
         :height (* (the-child :width) +phi+))
   
    (length-dimension :type 'parallel-dimension
                      :character-size (/ (the box length) 20)
                      :start-point (the box (vertex :top :left :front))
                      :end-point (the box (vertex :top :right :rear)))))

  (generate-sample-drawing :object-roots (make-object 'parallel-dimension-sample))

@end cartouche 
@end smallexample

